{
  "name": "canvas-ai",
  "version": "1.0.0",
  "type": "module",
  "description": "AI-powered visual canvas workspace",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "cd client && npm run dev",
    "server": "cd server && node index.js",
    "install-all": "npm install && cd client && npm install && cd ../server && npm install"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "cheerio": "^1.2.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "multer": "^1.4.5-lts.1",
    "openai": "^6.22.0",
    "pdf-parse": "^1.1.1",
    "serverless-http": "^3.2.0",
    "uuid": "^11.0.3",
    "youtube-transcript": "^1.2.1",
    "youtubei.js": "^16.0.1"
  },
  "devDependencies": {
    "netlify-cli": "^17.37.2"
  }
}
